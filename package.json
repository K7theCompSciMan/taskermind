		"@types/dotenv": "^8.2.0",
		"@types/eslint": "8.56.0",
		"@types/express": "^4.17.21",
		"@types/helmet": "^4.0.0",
		"@types/http-status-codes": "^1.2.0",
		"@types/uuid": "^9.0.8",
